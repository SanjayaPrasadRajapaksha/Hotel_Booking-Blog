<style>
    .chat-wrapper{
        width: 50px;
        height: auto;
        min-height: 200px;
        position: fixed;
        right: 0;
        top: 200px;
        z-index: 500;
        background-color: #4D44DB;
    }
    ul{
        list-style: none;
    }
    ul li{
        width: 100%;
        height: 50px;
